{
  "manifest_version": 2,
  "name": "C Clear",
  "description": "This extension clears cache and cookies",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_title": "Clear cache and cookies"
  },
  "background": {
  "scripts": ["background.js"],
   "persistent": false
  },
  "permissions": [
    "tabs",
    "browsingData"
  ]
}
